    <CubeBlocks>
        <Definition xsi:type="MyObjectBuilder_SurvivalKitDefinition">
            <Id>
                <TypeId>SurvivalKit</TypeId>
                <SubtypeId>EmergencySurvivalKit</SubtypeId>
            </Id>
            <DisplayName>Emergency Survival Kit</DisplayName>
